# Supabase AUTH & DB CVPHOTO
NEXT_PUBLIC_SUPABASE_URL=YOUR_SUPABASE_URL
NEXT_PUBLIC_SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY
SUPABASE_SERVICE_ROLE_KEY=YOUR_SUPABASE_SERVICE_ROLE_KEY

# Stripe TEST KEY ENV. CVPHOTO
STRIPE_TEST_SECRET_KEY=YOUR_STRIPE_TEST_SECRET_KEY
# Stripe LIVE PROD KEYS CVPHOTO
STRIPE_SECRET_KEY=YOUR_STRIPE_SECRET_KEY

# Polar Payment API CVPHOTO
# Get API key from https://polar.sh/dashboard
POLAR_ACCESS_TOKEN=YOUR_POLAR_ACCESS_TOKEN
# Resend.com Email Service for ProHeadshots
# Get API key from https://resend.com/api-keys
RESEND_API_KEY=YOUR_RESEND_API_KEY
NOREPLY_EMAIL=noreply@proheadshots.com
# Environment CVPHOTO
ENVIRONMENT=DEVELOPMENT

# Outrank.so Webhook Integration
OUTRANK_WEBHOOK_ACCESS_TOKEN=your_secure_outrank_access_token_here

# Supabase S3 CVPHOTO
SUPABASE_S3_ACCESS_KEY_ID=YOUR_SUPABASE_S3_ACCESS_KEY_ID
SUPABASE_S3_SECRET_ACCESS_KEY=YOUR_SUPABASE_S3_SECRET_ACCESS_KEY

# Replicate AI API - ProHeadshots Phase 1
# Get API token - https://replicate.com/account/api-tokens
REPLICATE_API_TOKEN=YOUR_REPLICATE_API_TOKEN

# RunPod API - ProHeadshots Phase 2 (for scaling)
# Get API key - https://runpod.io/console/user/settings
RUNPOD_API_KEY=YOUR_RUNPOD_API_KEY

# This can be any random string, it is used to secure the webhook endpoint. (Could be anything, e.g. "ANYSECRET123")
APP_WEBHOOK_SECRET=YOUR_APP_WEBHOOK_SECRET

# Site URL for redirects and callbacks (use http://localhost:3000 for local development and yourdomain.com for live prod)
NEXT_PUBLIC_SITE_URL=http://localhost:3000

# Service Provider Configuration - ProHeadshots
# AI_PROVIDER: 'replicate', 'runpod', or 'unified' (default: replicate)
AI_PROVIDER=replicate
# Primary and secondary providers for unified service (Phase 2)
AI_PRIMARY_PROVIDER=replicate
AI_SECONDARY_PROVIDER=runpod
# Enable/disable services (default: true)
AI_ENABLED=true
AI_FALLBACK_ENABLED=true
# PAYMENT_PROVIDER: 'stripe' or 'polar' (default: polar)
PAYMENT_PROVIDER=polar
PAYMENT_ENABLED=true

# Polar Webhook Secret (optional, for webhook signature verification)
POLAR_WEBHOOK_SECRET=YOUR_POLAR_WEBHOOK_SECRET



